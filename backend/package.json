{
  "name": "backend",
  "version": "0.1.0",
  "description": "HR analytics backend (upload, normalization, dashboards)",
  "main": "dist/server.js",
  "scripts": {
    "start": "node -r dotenv/config dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc -p tsconfig.json",
    "migrate": "knex --knexfile src/db/knexfile.ts migrate:latest",
    "test": "vitest",
    "fixtures:generate": "ts-node src/scripts/generateFixtures.ts",
    "seed:fixtures": "ts-node src/scripts/seed.ts",
    "ci": "npm run build && npm run test"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@fastify/websocket": "^11.0.1",
    "@prisma/client": "^7.1.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "exceljs": "^4.4.0",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.3",
    "knex": "^3.1.0",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "node-cache": "^5.1.2",
    "node-fetch": "^3.3.2",
    "pg": "^8.16.3",
    "pino": "^10.1.0",
    "pino-http": "^11.0.0",
    "sqlite3": "^5.1.7",
    "uuid": "^11.0.3",
    "ws": "^8.18.3",
    "xlsx": "^0.18.5",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.10.1",
    "@types/supertest": "^6.0.3",
    "@types/ws": "^8.18.1",
    "cross-env": "^10.1.0",
    "dotenv-cli": "^11.0.0",
    "prisma": "^7.1.0",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.15"
  }
}
